<% if projects.size > 0 %>
  <h3> Coincidencias en contenido: <%= projects.size %> </h3>
  <ol class="container">
    <% projects.order(end_date: :desc, rating: :desc).each do |project| %>
      <%= render partial: 'projects/preview', locals: { project: project, mines: false } %>
    <% end %>
  </ol>
<% end %>
